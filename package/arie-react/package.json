{
  "name": "arie-js",
  "version": "1.0.0",
  "description": "",
  "author": "Chandler Chappell <chan@atlrdsgn.com>",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chvndler/arie.js.git"
  },
  "main": "./dist/arie.mjs",
  "module": "./dist/arie.js",
  "exports": {
    ".": {
      "require": "./dist/arie.mjs",
      "import": "./dist/arie.umd.js"
    }
  },
  "typings": "./dist/arie-js.d.ts",
  "types": "./dist/arie-js.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build && dts-bundle-generator --config ./dts-bundle-generator.config.ts",
    "test": "vitest",
    "clean": "turbo run clean && rm -rf node_modules",
    "pretty": "prettier --write \"**/*.{ts,tsx,md}\"",
    "release": "yarn build && npm publish --access public"
  },
  "keywords": [],
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@types/node": "^20.3.2",
    "@types/react": "^18.2.12",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "@vitejs/plugin-react": "^4.0.2",
    "dts-bundle-generator": "^8.0.1",
    "eslint": "^8.44.0",
    "prettier": "^3.0.0",
    "react": "^18.2.0",
    "tslib": "^2.6.0",
    "turbo": "^1.10.7",
    "typescript": "^5.1.3",
    "vite": "^4.4.1",
    "vite-plugin-dts": "^3.1.1",
    "vitest": "^0.34.0"
